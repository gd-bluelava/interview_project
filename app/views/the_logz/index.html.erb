<%= render partial: 'shared/menu' %>

<h1>The Logz Dashboard</h1>

<table>
  <thead>
    <tr>
      <th>Query</th>
      <th>Answer</th>
      <th>Timestamp</th>
    </tr>
  </thead>
  <tbody>
    <% @logs.each do |log| %>
    <tr>
      <td><%= log.query %></td>
      <td><%= log.answer %></td>
      <td><%= log.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>
